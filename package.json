    "lint": "eslint src test docs",
    "docs": "vue-cli-service serve --port 9999 docs/main.js",
    "docs_build": "vue-cli-service build docs/main.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "main": "dist/evui.umd.js",
  "dependencies": {
