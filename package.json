    "node-sass": "^4.12.0",
    "raw-loader": "^4.0.1",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.7.0",
    "stylelint-order": "^4.1.0",
    "stylelint-webpack-plugin": "^2.1.0",
