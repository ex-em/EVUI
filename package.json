{
  "name": "evui",
  "version": "4.0.0-11",
  "description": "A EXEM Library project",
  "author": "exem <dev_client@ex-em.com>",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src docs",
    "docs": "vite serve --config vite.config.js --port 9999",
    "build:lib": "vite build --config vite.config.lib.js && rimraf ./dist/demo.html",
    "build:docs": "vite build --config vite.config.js --mode production"
  },
  "files": ["dist"],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    },
    "./style": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@vitejs/plugin-vue": "^5.1.4",
    "bignumber.js": "^9.1.2",
    "dayjs": "^1.10.4",
    "korean-regexp": "^1.0.10",
    "sass": "^1.79.3",
    "vite": "^5.4.8",
    "vue": "^3.5.8",
    "vue-resize-observer": "^2.0.15",
    "vue-router": "^4.0.0-0",
    "vue3-observe-visibility": "^1.0.1",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@vue/compiler-sfc": "^3.5.8",
    "@vue/test-utils": "^2.0.0-0",
    "axios": "^1.7.7",
    "codesandbox": "^0.0.0",
    "eslint": "^8.57.1",
    "eslint-config-exem": "^3.0.3",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-vue": "^8.7.1",
    "gh-pages": "^6.1.1",
    "highlight.js": "^10.2.0",
    "lodash-es": "^4.17.15",
    "marked": "^14.1.2",
    "stylelint": "^16.9.0",
    "stylelint-order": "^6.0.4",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "vue": "*"
  }
}
