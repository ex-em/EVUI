{
  "name": "exem-evui-library",
  "description": "A EXEM Library project",
  "author": "exem <dev_client@ex-em.com>",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "devDependencies": {
    "@eslint/js": "^8.56.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "nx": "19.0.4",
    "prettier": "^3.2.5",
    "typescript-eslint": "^7.12.0",
    "eslint-plugin-vue": "^9.26.0"
  },
  "scripts": {
    "build": "nx run-many --target=build --projects=@evui/*",
    "types-check": "nx run-many --target=type-check --projects=@evui/*",
    "storybook": "nx run @evui/storybook:storybook",
    "graph": "nx graph",
    "reset": "nx reset"
  }
}
