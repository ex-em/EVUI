{
  "name": "evui",
  "version": "4.0.0-1",
  "description": "A EXEM Library project",
  "author": "exem <dev_client@ex-em.com>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "build:lib": "vite build --target lib --name evui src/main.js && rimraf ./dist/demo.html",
    "build:lib_min": "vite build --formats umd-min --target lib --name evui src/main.js && rimraf ./dist/demo.html",
    "test:unit": "vite test:unit",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src docs",
    "docs": "vite serve --port 9999",
    "docs_build": "vite build --mode production"
  },
  "files": [
    "dist",
    "src"
  ],
  "main": "dist/evui.umd.js",
  "dependencies": {
    "@vitejs/plugin-vue": "^5.1.4",
    "bignumber.js": "^9.1.2",
    "dayjs": "^1.10.4",
    "korean-regexp": "^1.0.10",
    "sass": "^1.79.3",
    "vite": "^5.4.8",
    "vue": "^3.5.8",
    "vue-resize-observer": "^2.0.15",
    "vue-router": "^4.0.0-0",
    "vue3-observe-visibility": "^0.1.2",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@vue/compiler-sfc": "^3.5.8",
    "@vue/test-utils": "^2.0.0-0",
    "axios": "^1.7.7",
    "codesandbox": "^0.0.0",
    "eslint": "^8.57.1",
    "eslint-config-exem": "^3.0.3",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-vue": "^8.7.1",
    "gh-pages": "^6.1.1",
    "highlight.js": "^10.2.0",
    "lodash-es": "^4.17.15",
    "marked": "^14.1.2",
    "stylelint": "^16.9.0",
    "stylelint-order": "^6.0.4",
    "typescript": "^5.0.0"
  }
}
