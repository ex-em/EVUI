{
  "plugins": [
    "node_modules/jsdoc-vuejs"
  ],
  "recurseDepth": 10,
  "source": {
    "include": [
      "README.md",
      "src"
    ],
    "includePattern": ".(vue|js)$",
    "excludePattern": "(docs|mock|images)"
  },
  "opts": {
    "destination": "./src/docs",
    "encoding": "utf8",
    "recurse": true
  },
  "jsdoc-vuejs": {
    "followImports": true
  }
}
